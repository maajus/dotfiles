URxvt.termName: rxvt
URxvt.transparent: true
URxvt.shading:       20 
URxvt.inheritPixmap: False
URxvt.scrollBar: false
URxvt.saveLines: 10000
URxvt.urlLauncher: firefox
URxvt.cursorBlink: true
URxvt.geometry: 100x35

URxvt.perl-ext-common: default,keyboard-select,clipboard,url-select
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keysym.M-c:   perl:clipboard:copy
URxvt.keysym.M-v:   perl:clipboard:paste
URxvt.keysym.M-C-v: perl:clipboard:paste_escaped
URxvt.keysym.M-u: perl:url-select:select_next
URxvt.iso14755: False
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste


Xft.dpi:        96
Xft.antialias:  true
!Xft.rgba:       rgb
Xft.hinting:    true
!Xft.hinstyle:   hintslight
!URxvt.letterSpace:	 -2

URxvt.boldfont: xft:Ubuntu Mono derivative Powerline Bold:pixelsize=16:antialias=true
!URxvt.boldfont: xft:Consolas for Powerline Bold:pixelsize=13:antialias=true
!urxvt.font: xft:Consolas for Powerline:pixelsize=13:antialias=true, \
urxvt.font: xft:Ubuntu Mono derivative Powerline:pixelsize=16:antialias=true, \
            xft:WenQuanYi Bitmap Song:size=8, \
            xft:FreeSerif:style=Regular, \
            xft:unifont:style=Medium:antialias=false

!URxvt.boldfont: xft:Inconsolata for Powerline:bold:pixelsize=16:antialias=true
!URxvt.font: xft:Inconsolata for Powerline:pixelsize=16:antialias=true
!URxvt.boldfont: xft:Inconsolata-dz for Powerline:bold:pixelsize=13:antialias=true
!URxvt.font: xft:Inconsolata-dz for Powerline:pixelsize=12:antialias=true


!rxvt.depth: 32
!URxvt.background: rgba:2400/2400/2400/f4aa
!urxvt.foreground: rgba:ffff/ffff/ffff/cccc
URxvt.tintColor: #D4D4D4
URxvt.borderLess: false
URxvt.InternalBorder: 0
URxvt.externalBorder: 0
URxvt.borderColor: #282828
!URxvt*iconFile:    //home/justas/.icons/AwOken/clear/24x24/apps/terminal.png
